{
  "name": "WebTester",
  "description": "WebTester",
  "version": "1.0",
  "permissions": ["nativeMessaging"],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "all_frames": true,
      "css": ["webtester.css"],
      "js": ["jquery-1.11.2.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "WebTester"
  },
  "icons": {
    "16": "icon.png"
  },
  "manifest_version": 2
}
